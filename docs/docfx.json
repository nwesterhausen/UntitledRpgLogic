{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/UntitledRpgLogic.Core/UntitledRpgLogic.Core.csproj",
            "src/UntitledRpgLogic.Extensions.Common/UntitledRpgLogic.Extensions.Common.csproj",
            "src/UntitledRpgLogic.Extensions.Godot/UntitledRpgLogic.Extensions.Godot.csproj",
            "src/UntitledRpgLogic.Extensions.Logging/UntitledRpgLogic.Extensions.Logging.csproj",
            "src/UntitledRpgLogic.Infrastructure.Configuration/UntitledRpgLogic.Infrastructure.Configuration.csproj",
            "src/UntitledRpgLogic.Infrastructure.Data/UntitledRpgLogic.Infrastructure.Data.csproj",
            "src/UntitledRpgLogic.Resources/UntitledRpgLogic.Resources.csproj",
            "src/UntitledRpgLogic.Services/UntitledRpgLogic.Services.csproj",
            "src/UntitledRpgLogic.StateMachines/UntitledRpgLogic.StateMachines.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "memberLayout": "separatePages",
      "includeExplicitInterfaceImplementations": true,
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
		"overwrite": [
			{
				"files": [
					"namespaces/**.md"
				]
			}
		],
    "output": "_site",
    "template": [
      "default",
      "templates/singulinkfx"
    ],
    "globalMetadata": {
      "_appName": "UntitledRpgLogic Docs",
      "_appTitle": "UntitledRpgLogic Docs",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
